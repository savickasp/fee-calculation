{
    "name": "savickasp/fee-calculation",
    "description": "",
    "keywords": [],
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=7.0",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "2.16",
        "phpunit/phpunit": "^6.5",
        "symfony/var-dumper": "^5.0"
    },
    "config": {
        "sort-packages": true,
        "bin-dir": "bin"
    },
    "scripts": {
        "phpunit": "phpunit",
        "fix-cs": "php-cs-fixer fix",
        "test-cs": "php-cs-fixer fix --dry-run -v",
        "test": ["@phpunit", "@test-cs"]
    }
}
